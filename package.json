    "Hugo Virgen (https://github.com/virgenherrera)"
  ],
  "engines": {
    "node": ">=18.12.0 <19",
    "npm": ">=8.6.0"
  },
  "scripts": {
    "test": "run-s -l cleanup test:* build:*",
    "dev": "vite",
    "preview": "vite preview",
    "UT:develop": "jest --watch --maxWorkers=1",
    "UT:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "build": "run-s -l build:*",
    "build:tsc": "tsc",
    "build:vite": "vite build",
    "test:static": "eslint src --ext=ts",
    "test:unit": "jest",
    "cleanup": "rimraf coverage/ dist/"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  }
